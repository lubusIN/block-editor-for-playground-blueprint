{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "lubus/unzip",
	"version": "0.1.0",
	"title": "Unzip",
	"category": "steps",
	"description": "Unzip a zip file.",
	"example": {},
